{
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "env": {
      "VITE_SUPABASE_URL": "your-supabase-project-url",
      "VITE_SUPABASE_ANON_KEY": "your-supabase-anon-key"
    }
  }